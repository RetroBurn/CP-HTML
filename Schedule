	<script>			
function Baby() {  <!--Everything underneath this is connected to the classes I gave to each of my courses up above. -->
	var Lemon = document.getElementById("Classroom").value;
	switch (Lemon) {
		case 'Math':
			console.log('Math is a bit difficult this year');
			var Oh = "Math";
			break;
		case 'Religion':
			console.log('Religion is really boring');
			var Oh = "Rel";
			break;
		case 'Science':
			console.log('Science is really fun');
			var Oh = "Science";
			break;
		case 'French':
			console.log('French is OK');
			var Oh = "French";
			break;
		case 'Music':
		     console.log('Music is hard');
			 var Oh = "Flute";
			break;
		case 'English':
			console.log('English is OK');
			var Oh = "English";
			break;	
		case 'Computer Programming':
			console.log('Computer Programming is hard but fun');
			var Oh = "Programming"; 
			break;
		case 'History':
			console.log('History is not that interesting this year');
			var Oh = "History";
			break;
		case 'Gym':
			console.log('Gym is really fun');		
			var Oh = "Gym";
			break;
		case 'Speech':
			console.log('Speech is Hell');
			var Oh = "Hell";
			break;
		case 'Advisors':
			console.log('Advisors is relaxing');
			var Oh = "Relaxing Time";
			break;
		case 'HR':
			console.log('HoomRoom is relaxing');
			var Oh = "Free Time";
			break;	
		case 'LA':
			console.log('LA is interesting');
			var Oh = "Comfy Chairs";
			break;
		case 'Late Start':
			console.log('I get to sleep in');
			var Oh = "Sleep In";
			break;
		case 'Mass':
			console.log('Somethimes I fall asleep');
			var Oh = "I get Very Sleepy";
			break;
		case 'Early Dismissal':
			console.log('Early Gaming');
			var Oh = "Early Gaming";
			break;
	}
	var Orange = document.getElementsByClassName(Oh);
	for (x = 0; x < Orange.length; x++) {
		Orange[x].style.backgroundColor = "yellow";  <!--This line of code right here colors the subject when it is searched up -->
	}
		
		
}

	</script>
